@model SanjivaniModalView.Employees

@{
    ViewBag.Title = "NewEmployee";
    Layout = "~/Views/Shared/_LayoutSanjivani.cshtml";
}

<style>
    label:not(.form-check-label):not(.custom-file-label) {
        font-weight: 400;
    }
</style>
<div class="content-header">
    <div class="container-fluid">
        <div class="row mb-2">
            <div class="col-sm-12">
                <h1 class="m-0 text-danger" style="text-align: center;font-weight: 1000;color: #a9a0a1!important;">EMPLOYEE-INFO</h1>
            </div><!-- /.col -->

        </div><!-- /.row -->
    </div><!-- /.container-fluid -->
</div>


<section class="content">
    <div class="container-fluid">
        <!-- Small boxes (Stat box) -->
        <div class="row">
            <div class="col-md-12">
                <!-- Custom Tabs -->
                @Html.HiddenFor(model => model.CustId)

            </div>
            <div class="col-12 col-sm-12">
                <div class="card card-primary card-tabs">
                    <div class="card-header p-0 pt-1" style="background-color: #030358;">
                        <ul class="nav nav-tabs" id="custom-tabs-one-tab" role="tablist">
                            <li class="nav-item" id="one">
                                <a class="nav-link active" id="T1" data-toggle="pill" href="#tab_1" role="tab" aria-controls="custom-tabs-one-home" aria-selected="false">Personal Info</a>
                            </li>
                            <li class="nav-item" id="two">
                                <a class="nav-link " id="T2" data-toggle="pill" href="#tab_2" role="tab" aria-controls="custom-tabs-one-profile" aria-selected="true">Bank</a>
                            </li>
                            <li class="nav-item" id="three">
                                <a class="nav-link" id="T3" data-toggle="pill" href="#tab_3" role="tab" aria-controls="custom-tabs-one-messages" aria-selected="false">Document</a>
                            </li>
                          
                        </ul>
                    </div>
                    <div class="card-body" style="border: 1px solid;background-color: ghostwhite;">
                        <div class="tab-content" id="custom-tabs-one-tabContent">
                            <div class="tab-pane fade active show" id="tab_1" role="tabpanel" aria-labelledby="custom-tabs-one-home-tab">

                            </div>
                            <div class="tab-pane fade" id="tab_2" role="tabpanel" aria-labelledby="custom-tabs-one-profile-tab">

                            </div>
                            <div class="tab-pane fade" id="tab_3" role="tabpanel" aria-labelledby="custom-tabs-one-messages-tab">

                            </div>
                           
                        </div>
                    </div>
                    <!-- /.card -->

                    <div class="col-md-12" style="text-align:center">
         
                        
                        <a type="submit" class="btn btn-danger" style="color:white" href="@Url.Action("DeleteEmp","Director")"><i class="fa fa-trash"></i> Delete</a>
                        <a type="submit" class="btn btn-danger" style="color:white" href="@Url.Action("Employee","Director")"><i class="fa fa-times"></i> Close</a>
                    </div>


                </div>
            </div>
        </div>
    </div>
</section>
