@model SanjivaniModalView.DirectorBusinessModel
@using (Html.BeginForm("_SetDirectorDocument", "Director", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    @Html.AntiForgeryToken()
    <div class="box box-danger" id="loading-example">
     
        <div class="box-body no-padding">
            @Html.HiddenFor(model => model.CustId)
            <div class="row">
                <div class="form-group col-md-3">
                    <label for="exampleInputFile">Upload Profile photo</label>
                    <input type="file" name="postedFile" class="form-control" id="fileuploaded" style="background-color: transparent;border: none;" accept="image/png,image/jpeg,image/jpg,application/pdf" required />
                </div>
                <div class="form-group col-md-3">
                    <label for="exampleInputFile">Upload Company PAN Card</label>
                    <input type="file" name="postedFile" class="form-control" id="fileuploaded1"style="background-color: transparent;border: none;" accept="image/png,image/jpeg,image/jpg,application/pdf"/>
                </div>
                <div class="form-group col-md-3">
                    <label for="exampleInputFile">Upload Adhaar Card </label>
                    <input type="file" name="postedFile" class="form-control" id="fileuploaded2" style="background-color: transparent;border: none;" accept="image/png,image/jpeg,image/jpg,application/pdf" />
                </div>
                <div class="form-group col-md-3">
                    <label for="exampleInputFile">Upload Light Bill</label>
                    <input type="file" name="postedFile" class="form-control" id="fileuploaded3" style="background-color: transparent;border: none;" accept="image/png,image/jpeg,image/jpg,application/pdf" />
                </div>
                <div class="form-group col-md-3">
                    <label for="exampleInputFile">Upload Passport</label>
                    <input type="file" name="postedFile" class="form-control" id="fileuploaded4" style="background-color: transparent;border: none;" accept="image/png,image/jpeg,image/jpg,application/pdf" />
                </div>

            </div><!-- /.row - inside box -->
        </div><!-- /.box-body -->

    </div>
    <button type="submit" id="ddsave" class="btn btn-danger"><i class="fa fa-save"></i>Save</button>
}
<div class="box box-danger" id="loading-example" style="margin-top:10px">

    <div class="box-body no-padding" style="background-color: whitesmoke;">
        <div class="row" id="Doucment">


        </div><!-- /.row - inside box -->
    </div><!-- /.box-body -->

</div>
