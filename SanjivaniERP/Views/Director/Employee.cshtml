            "bFilter": false,
            "bSort": true,
            "bInfo": true,
            "bAutoWidth": false
        });
    });
    function write_to_excel() {
        $("#example1").table2excel({
            name: "Table2Excel",
            filename: "myFileName",
            fileext: ".xls"
        });
    }
    function DownloadExcel() {
        $.ajax({
            type: "POST",
            url: "/Partner/ExcelExportCPList",
            cache: false,
